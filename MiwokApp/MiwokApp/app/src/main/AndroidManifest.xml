<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.technomaniacs.android.miwokapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.MiwokApp"
        android:extractNativeLibs="true"
        >
        <activity
            android:name=".PhrasesActivity"
            android:label="@string/phrases"
            android:exported="true" />
        <activity
            android:name=".ColorsActivity"
            android:label="@string/colors"
            android:exported="true"
            />
        <activity
            android:name=".FamilyMembersActivity"
            android:label="@string/family_Member"
            android:exported="true" />
        <activity
            android:name=".NumbersActivity"
            android:label="@string/numbers"
            android:exported="true" />
        <activity
            android:name=".MainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>